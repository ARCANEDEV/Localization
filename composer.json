{
    "name": "arcanedev/localization",
    "description": "Localization Package for Laravel",
    "keywords": ["laravel", "localization", "localisation"],
    "homepage": "https://github.com/ARCANEDEV/Workbench",
    "license": "MIT",
    "authors": [
        {
            "name": "ARCANEDEV",
            "email": "arcanedev.maroc@gmail.com",
            "homepage": "https://github.com/ARCANEDEV"
        }
    ],
    "require": {
        "php" : ">=5.5.9",
        "arcanedev/support": "~3.6.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.1",
        "phpunit/phpunit": "~4.0",
        "phpunit/phpcov": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Arcanedev\\Localization\\": "src/"
        },
        "files": ["helpers.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Arcanedev\\Localization\\Tests\\": "tests/"
        }
    },
    "suggest": {
        "ext-intl": "Use Intl extension for 'Locale' class (an identifier used to get language)."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "stable"
}
